{
  "manifest_version": 2,
  "name": "Tab FZF",
  "description": "List current tabs in a popup.",
  "version": "1.0.0",
  "permissions": [
    "tabs"
  ],
  "icons": {
    "16": "icons/tabfzf-16.png",
    "32": "icons/tabfzf-32.png",
    "48": "icons/tabfzf-48.png"
  },
  "browser_action": {
    "default_title": "Tab FZF",
    "default_icon": {
      "16": "icons/tabfzf-16.png",
      "32": "icons/tabfzf-32.png",
      "48": "icons/tabfzf-48.png"
    },
    "default_popup": "popup/index.html"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F"
      },
      "description": "Open Tab FZF"
    }
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "tabfzf@example.local"
    }
  }
}
